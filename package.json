{
  "name": "myprojectname-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_DIST_DIR=.next-dev next dev -p 50400",
    "dev:test": "APP_ENV=test bun run db:reset && NEXT_DIST_DIR=.next-test APP_ENV=test next dev -p 50410",
    "build": "next build",
    "build:test": "APP_ENV=test bun run db:reset && NEXT_DIST_DIR=.next-test APP_ENV=test next build",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "start:local": "next start -p 50400",
    "start:test": "APP_ENV=test bun run db:reset && NEXT_DIST_DIR=.next-test APP_ENV=test next start -p 50410",
    "lint": "eslint",
    "test": "APP_ENV=test bun run db:reset && APP_ENV=test playwright test --config=test.config.ts",
    "test:ui": "playwright test --ui --config=test.config.ts",
    "test:headed": "playwright test --headed --config=test.config.ts",
    "test:debug": "playwright test --debug --config=test.config.ts",
    "test:failed": "playwright test --last-failed --config=test.config.ts",
    "test:report": "playwright show-report",
    "db:setup": "bun run scripts/db/index.ts setup",
    "db:clean": "bun run scripts/db/index.ts clean",
    "db:seed": "bun run scripts/db/index.ts seed",
    "db:reset": "bun run scripts/db/index.ts reset",
    "db:generate": "bunx prisma generate",
    "db:push": "bunx prisma db push",
    "db:migrate": "bunx prisma migrate dev",
    "db:migrate:deploy": "bunx prisma migrate deploy",
    "db:studio": "bunx prisma studio",
    "db:studio:test": "APP_ENV=test bunx prisma studio",
    "setup": "bun run scripts/setup.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.12",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.561.0",
    "next": "^16.0.10",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^25.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "playwright": "^1.53.2",
    "@playwright/test": "^1.55.1",
    "prisma": "^7.1.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "bun prisma/seed.ts"
  }
}
